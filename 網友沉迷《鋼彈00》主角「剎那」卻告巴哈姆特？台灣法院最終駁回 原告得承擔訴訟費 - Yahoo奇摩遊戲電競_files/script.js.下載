var PageIn = 1;
var LastPos = 0;
var StopLoadMore = StopLoadMore || false;
var DataLoading = false;
var LastPostTop = 0;
__UA = navigator.userAgent;
__WinW = $(window).width();
__WinH = window.innerHeight;
function intval($v) {
	$v = parseInt($v, 10);
	return (isNaN($v)?0:$v);
}
function $YAD() {
    $.getScript('https://s.yimg.com/ja/ap/pageassistant/js/pageAssist1412651471.js');	
}
function SiteSearch() {
	var KW = $('#KeyWords').val();
	$('#SiteSearchForm input[name="q"]').val(KW);
	gtag('event', '搜尋_遊戲電競搜尋', {'event_category': '搜尋', 'event_label': KW});
    document.location.href="/search/" + encodeURIComponent(KW);
}
function WebSearch() {
	var KW = $('#KeyWords').val();
	$('#web-search input[name="p"]').val(KW);
	gtag('event', '搜尋_網頁搜尋', {'event_category': '搜尋', 'event_label': KW});
	document.location.href="https://tw.search.yahoo.com/search?p=" + encodeURIComponent(KW) + "&ei=UTF8&fr=uh3_games_web_gs";
}
function addCommas(nStr) {
	nStr += '';
	x = nStr.split('.');
	x1 = x[0];
	x2 = x.length > 1 ? '.' + x[1] : '';
	var rgx = /(\d+)(\d{3})/;
	while (rgx.test(x1)) {
	  x1 = x1.replace(rgx, '$1' + ',' + '$2');
	}
	return x1 + x2;
}
function getXSRF() {
	var key = uuidv4();
	Cookies.set('XSRF-TOKEN', key, { expires: 7 });
	return key;
}
function CutWord($v, $len) { // 20141117 by STAR
	$len = intval($len);
	if ($len < 2) { $len = 16; } //16 Eng Letter = 8 CJK Word
	if (typeof($v) !== "string") { return ''; }
	$v = $.trim($v.replace(/[\r\n]/g," "));
	$vlen = $v.length;
	if ($vlen <= $len) { return $v; } 
	var returnword = '';
	var wordCount = 0;
	var cjkREx = /[\u3400-\u4db5\u4e00-\u9fa5\uf900-\ufa2d\u20000-\u2a6d6\u2a700-\u2b734\u2b740-\u2b81d\u2f800-\u2fa1d]/; //DataFrom http://www.unicode.org/charts/unihangridindex.html
	var wordBreakRegEx = /\W/;
	for (var i = 0; i < $vlen; i++) {
		var $Char = $v.charAt(i);
		var CharURI = '';
		try {
			CharURI = encodeURIComponent($Char);
		} catch(e){}
		if (CharURI.length >= 9) {
			wordCount += 2;
			returnword += $Char.toString();
			if (wordCount >= $len) {
				break;	
			}
		} else {
			wordCount++;
			returnword += $Char.toString();
			if (wordCount >= $len) {
				break;	
			}
		}
	}
	return returnword;
}
function DisableSelection(obj) {
	if (obj && obj.jquery) {
		obj.attr('unselectable', 'on')
        .css('user-select', 'none')
        .css('-moz-user-select', 'none')
        .css('-khtml-user-select', 'none')
        .css('-webkit-user-select', 'none')
        .on('selectstart', false)
        .on('contextmenu', false)
        .on('keydown', false)
        .on('mousedown', false);
	}
}
$YG = {
	'init': function() {
		$.post('/block/CCPA.php', {}, function(DH) {
			if (DH != '') { $('#CCPAPre').after(DH); }
		});
		$("#KeyWords").on('keydown', function(e) {
			if (e.keyCode == 13) {
				($("#KeyWords").val() == '')?$("#KeyWords").focus():SiteSearch();
			}
		});
		$("[data-ga]").click(function(){
			//var gadata = JSON.parse($(this).data('ga'));
			var gadata = JSON.parse($(this).data('ga').replace(/'/g, '"'));
			if (Array.isArray(gadata)) {
				if (gadata.length == 2) {
					gtag('event', gadata[1], {'event_category': gadata[0]});
				} else if (gadata.length > 2) {
					gtag('event', gadata[1], {'event_category': gadata[0], 'event_label': gadata[2]});
				}
			}
			
		})
        $YG.MenuStart();
		if ($('#content-l .post-list .item').length === 0) {
			if ($('#video-more .item').length > 0) {
				LastPostTop = Math.max(0, $('#video-more .item').offset().top - $(window).height());
			} else {
				LastPostTop = Math.max(0, $('#maincontainer').offset().top - $(window).height());
			}
		} else {
			LastPostTop = Math.max(0, $('#content-l .post-list .item:last').offset().top - $(window).height());
		}
		$(".dotdotdot").truncate({multiline: true});
		$(".dotdotmore").truncate({multiline: true, token: '... <b>詳全文</b>'});
		$YG.MakeAD();
		$YG.SetZ();
		$YG.MakeComScore();
        $YG.MakeDotQ();
	},
	'MenuStart': function() {
		$('#ytm_nav li.hassub').each(function(){
			var $maxL = 1000 + $('#ytm_nav nav').offset().left - $(this).find('.sublist dl').width();
			var $mL = $(this).offset().left + ($(this).width() - $(this).find('.sublist dt').width())/2;
			$(this).find('.sublist dl').css({'margin-left': Math.min($maxL, $mL)});
		});
		$('#ytm_nav .sublist').hide().css({opacity: 1});
	},
	'SearchLoad': function() {
		DataLoading = true;
		var FD = {};
		FD.q = Nuuid;
		FD.t = SearchType;
		$.post('/search/load/data.php', FD, function(DH) {
			if (FD.t == '') {
				$('#search-list').html(DH);
			}
		});
	},
	LoadVideoMoreList: function(ST) {
		if (StopLoadMore) {
			return false;
		}
		if (DataLoading) {
			return false;
		}
		if ($('#video-more').length === 0) {
			return false;
		}
		if (ST <= LastPostTop) {
			return false;
		}
		DataLoading = true;
		$('#video-more .loader').show();
		var FD = {};
		FD.P = PageIn + 1;
		FD.uuid = MoreUUID;
		$.post('/ajax/readmore.php', FD, function(DH) {
			$('#video-more .GContent').append(DH);
			$(".dotdotdot").truncate({multiline: true});
			$(".dotdotmore").truncate({multiline: true, token: '... <b>詳全文</b>'});
			if ($('#video-more .item').length === 0) {
				LastPostTop = Math.max(0, $('#maincontainer').offset().top - $(window).height());
			} else {
				LastPostTop = Math.max(0, $('#video-more .item:last').offset().top - $(window).height());
			}
			PageIn++;
			DataLoading = false;
			$('#video-more .loader').hide();
		});
	},
	LoadMoreList: function(ST) {
		if (StopLoadMore) {
			return false;
		}
		if (DataLoading) {
			return false;
		}
		if ($('#content-l .post-list').length === 0) {
			if ($('#video-more').length > 0) {
				$YG.LoadVideoMoreList(ST);
			}
			return false;
		}
		if (ST <= LastPostTop) {
			return false;
		}
		DataLoading = true;
		$('#content-l .post-list .loader').show();
		var FD = {};
		FD.P = PageIn + 1;
		FD.uuid = Nuuid;
        FD.LoadType = LoadType;
		if (typeof(TipHome) == 'undefined') {
			TipHome =0;
		}
		FD.TipHome = TipHome;
		$.post('/ajax/list.php', FD, function(DH) {
			$('#content-l .post-list .GContent').append(DH);
			$('#content-l .post-list .GContent').find('[data-ga]').unbind('click').click(function(){
                //var gadata = JSON.parse($(this).data('ga'));
                var gadata = JSON.parse($(this).data('ga').replace(/'/g, '"'));
                if (Array.isArray(gadata)) {
                    if (gadata.length == 2) {
                        gtag('event', gadata[1], {'event_category': gadata[0]});
                    } else if (gadata.length > 2) {
                        gtag('event', gadata[1], {'event_category': gadata[0], 'event_label': gadata[2]});
                    }
                }	
            })
			$(".dotdotdot").truncate({multiline: true});
			$(".dotdotmore").truncate({multiline: true, token: '... <b>詳全文</b>'});
			if ($('#content-l .post-list .item').length === 0) {
				LastPostTop = Math.max(0, $('#maincontainer').offset().top - $(window).height());
			} else {
				LastPostTop = Math.max(0, $('#content-l .post-list .item:last').offset().top - $(window).height());
			}
			PageIn++;
			DataLoading = false;
			$('#content-l .post-list .loader').hide();
		});
	},
    'CheckItem': function(obj) {
		if (obj.hasClass('.Activity')) {
			$('#poll li.Activity').removeClass('Activity');
		} else {
			$('#poll li.Activity').removeClass('Activity');
			obj.addClass('Activity');
		}
	},
	SNSFB: function() {
		var WH = 612;
		var WW = 555;
		var Url = 'https://www.facebook.com/dialog/feed?app_id=' + FBappid + '&display=popup&show_error=yes&link=' + encodeURIComponent(ShareURL);
		var Pos = $YG.POPPos(WW, WH);
		window.open(Url, 'TWYahooStyleFBShare', config='toolbar=no,menubar=no,status=no,height=' + Pos.h + ',width=' + Pos.w + ',top=' + Pos.top + ',left=' + Pos.left);
	},
	POPPos: function(w, h) {
		var MultiScreenLeft = window.screenLeft !== undefined ? window.screenLeft : screen.left;
		var MultiScreenTop = window.screenTop !== undefined ? window.screenTop : screen.top;
		var width = window.innerWidth ? window.innerWidth : document.documentElement.clientWidth ? document.documentElement.clientWidth : screen.width;
		var height = window.innerHeight ? window.innerHeight : document.documentElement.clientHeight ? document.documentElement.clientHeight : screen.height;
		Pos = {'w': w, 'h': h, 'left': ((width / 2) - (w / 2)) + MultiScreenLeft, 'top': ((height / 2) - (h / 2)) + MultiScreenTop};
		return Pos;
	},
	'ScrollEvent': function() {
		var ST = $(window).scrollTop();
		$YG.ScrollFooter(ST);
		$YG.ScrollRightCol(ST);
        $YG.LoadMoreList(ST);
	},
	'ScrollFooter': function(ST) {
		if (!ST) {
			ST = $(window).scrollTop();
		}
		if (ST > 0) {
			$('footer').hide();
		} else {
			$('footer').show();
		}
	},
	'ScrollRightCol': function(ST) {
		if (!ST) {
			ST = $(window).scrollTop();
		}
		var LH = $('#content-l').height();
		var RH = $('#content-r').height();
		if (RH > LH) {
			$('#content-r').css({'margin-top': 0});
			return false;
		}
		var MaxT = LH - RH;
		var StartT = $('#content-r').offset().top + RH - $(window).height() - intval($('#content-r').css('margin-top'));
		if (ST > StartT) {
			var MT = Math.min(MaxT, (ST - StartT));
			$('#content-r').css({'margin-top': MT});
		} else {
			$('#content-r').css({'margin-top': 0});
		}
	},
	PageImageList: [],
    PageElementResize: function() {
		var ImgFull = 500;
		var ImgCenter = 100;
		var loopi = 0;
		$('#article-content img').each(function() {
			var tmp = {};
			var imgobj = $(this);
			imgobj.attr('data-imageid', loopi).data('imageid', loopi);
			tmp.src = imgobj.attr('src');
			var origwidth = intval(imgobj.data('orig-width'));
			if (origwidth > 0) {
				imgobj.css({'max-width': origwidth});
				tmp.width = origwidth;
			}
			$YG.PageImageList[loopi] = tmp;
			imgobj.imagesLoaded(function() {
				var width = imgobj.width();
				var pid = intval(imgobj.data('imageid'));
				$YG.PageImageList[pid].width = width;
				if (width >= ImgFull) {
					imgobj.removeAttr('width height').css({'width': '100%', 'height': ''}).addClass('zoom').click(function(event) { $YG.BuildPageGallery(event, pid); });	
				} else if (width > ImgCenter && width < ImgFull) {
					imgobj.addClass('zoom').click(function(event) { $YG.BuildPageGallery(event, pid); }).wrap("<figure></figure>");
				} else if (width < ImgCenter) {
					imgobj.addClass('no-zoom');
				}
			});
			loopi++;
		});
		var VAWidth = $('#article-content').width();
		$('#content-l article .audio_container, #content-l article .video-wrapper').find('iframe, audio, video, div').add('#article-content iframe, #article-content audio, #article-content video').each(function(){
			var origwidth = $(this).width();
			$(this).data('orig-width', origwidth).attr('data-orig-width', origwidth);
			var origheigh = $(this).height();
			var videorate = Math.round(origheigh * 100 / origwidth) / 100;
			$(this).data('rate', videorate).attr('data-rate', videorate);
			var NewHeight = VAWidth * videorate;
			$(this).removeAttr('width height').width(VAWidth).height(NewHeight);	
		});
		if ($('.twitter-tweet').length > 0) {
			$.getScript('https://platform.twitter.com/widgets.js');	
		}
		if ($('.instagram-media').length > 0) {
			$.getScript('//platform.instagram.com/en_US/embeds.js');
		}
    },
	'BuildPageGallery': function(event, index) {
		var SetID = 0;
		try {
			event.stopPropagation();
		} catch(e) {}
    	$.scrollLock();
		var $DH = '';
		var $IMG = '';
		if ($YG.PageImageList.length == 0) {
			return false;
		}
		var idx = 0;
        $.each($YG.PageImageList, function(k,v){
            if (v.width < 100) {
                return;
            }
			if (index == k) {
				SetID = idx;
			}
            $IMG += '<div class="item_img img_' + k + '" style="background-image:url(' + v.src + ')"></div>';
			idx++;
        });
        if ($IMG == '') {
            return false;
        }
		$DH = '<div id="block-overlay" onClick="$YG.RemovePageGallery()"></div><div id="post-gallery"><div id="main-show">' + $IMG + '</div><div id="list-show">' + $IMG + '</div><button id="block-close" type="button" onClick="$YG.RemovePageGallery()">&nbsp; 關&nbsp; 閉</button></div>';
		$('#maincontainer').after($DH);
		$('#main-show').slick({
		  slidesToShow: 1,
		  slidesToScroll: 1,
		  arrows: true,
		  fade: true,
		  asNavFor: '#list-show'
		});
		$('#list-show').slick({
		  slidesToShow: 6,
		  slidesToScroll: 1,
		  asNavFor: '#main-show',
		  dots: false,
		  arrows: false,
		  centerMode: false,
		  focusOnSelect: true
		});
		if (SetID > 0) {
			$('#list-show').slick('slickGoTo', SetID, true);
		}
	},
	'RemovePageGallery': function() {
		$('#block-overlay, #post-gallery').remove();
		$.scrollLock(false);
	},
	'POPVIDEO': function($EID) {
		$.scrollLock();
		var $DH = '<div id="block-overlay" onClick="$YG.RemovePOPVIDEO()"></div><div id="esport-embed"><button id="block-close" type="button" onClick="$YG.RemovePOPVIDEO()">&nbsp; 關&nbsp; 閉</button></div>';
		$('body').append($DH);
		$.post('/esports/load/embed.php', {'eid': $EID}, function(GetDH) {
			$('#esport-embed').prepend(GetDH);
		});
	},
	'RemovePOPVIDEO': function() {
		$('#block-overlay, #esport-embed').remove();
		$.scrollLock(false);
	},
	'SwitchRank': function(obj) {
		var $RTID = intval(obj.data('id'));
		$('#award .Activity').removeClass('Activity');
		obj.add('#RankList_' + $RTID).addClass('Activity');	
	},
	'SendPoll': function() {
		var FD = {'X-XSRF-TOKEN': getXSRF()};
		FD.PID = intval($('#POLLID').val());
		FD.IID = intval($('#poll .form li.Activity').data('id'));
		$.post('/ajax/poll.php', FD, $.noop, 'script');
	},
	'SetGame': function(obj) {
		$('#GameList .Activity').removeClass('Activity');
		obj.closest('li').addClass('Activity');
		var $url = '/esports/';
		if (obj.text() == '綜合賽事') {
			hash.game = "";
			$url +=  hash.ym;
		} else {
			hash.game = obj.text();
			$url +=  hash.ym + '/' + encodeURIComponent(hash.game);
		}
		History.pushState(null, null, $url);
		$YG.LoadESPORT();
	},
	'SetYM': function(obj) {
		$('#YMList .Activity').removeClass('Activity');
		obj.closest('li').addClass('Activity');
		$('#GameList .Activity').removeClass('Activity');
		$('#GameList li:eq(0)').addClass('Activity');
		hash.game = "";
		hash.ym = obj.data('ym');
		var $url = '/esports/' + hash.ym;
		History.pushState(null, "電競賽事 - Yahoo奇摩遊戲電競", $url)
		$YG.LoadESPORT();
	},
	'LoadESPORT': function() {
		$('#content-l .esport-list').html('<div class="loader">Loading</div>');
		var FD = {};
		FD.ym = hash.ym;
        FD.game = hash.game;
		$.post('/esports/load/data.php', FD, function(DH) {
			$('#content-l .esport-list').html(DH);
			$("#content-l .esport-list [data-ga]").click(function(){
				//var gadata = JSON.parse($(this).data('ga'));
				var gadata = JSON.parse($(this).data('ga').replace(/'/g, '"'));
				if (Array.isArray(gadata)) {
					if (gadata.length == 2) {
						gtag('event', gadata[1], {'event_category': gadata[0] });
					} else if (gadata.length > 2) {
						gtag('event', gadata[1], {'event_category': gadata[0], 'event_label': gadata[2]});
					}
				}	
			});
		});
	},
	'SetVideo': function($ID) {
		var SelTab = [];
		if (typeof($ID) != 'undefined') {
			SelTab = $('#LID_' + $ID);
		}
		if (SelTab.length == 0) {
			SelTab = $('#IDXLIVE .tab li').eq(0);
		}
		if (SelTab.length == 0) { return false; }
		$ID = SelTab.data('id');
		$('#IDXLIVE .videobox').text('');
		$(LiveEmbed[$ID]).width(1200).height(675).appendTo('#IDXLIVE .videobox');
		$('#IDXLIVE .tab li').removeClass('Activity');
		SelTab.addClass('Activity');
		return true;
	},
	'SetIdxESPROT': function(obj) {
		var gid = intval(obj.data('gid'));
		$('#idx-esports .Activity').removeClass('Activity');
		obj.add('#esport-' + gid).addClass('Activity');
	},
    ScrollToTop: function() {
        $(window).scrollTop(0);
    },
	'Debug': 0,
	'MakeAD': function() {    
		var ADWord = 'LREC{dest:LRECAD;asz:300x250},LREC3{dest:LREC3AD;asz:300x250}';
        if ($('#BOXMAST').length > 0) {
            ADWord += ',MAST{dest:MASTAD;asz:970xundefined}';
        }
        $.getScript('https://fc.yahoo.com/sdarla/php/client.php?l=' + ADWord + '&f=' + __SpaceID + ($YG.Debug > 0?'&debug=1':'') + '&npv=1&ref=' + encodeURIComponent(location.href) + '&_timestamp' + new Date().getTime());
		return true;
	},
	'ZConf': {},
	'SetZ': function() {
		if (__SpaceID === '') {
			return false;	
		}
		if ($.isEmptyObject($YG.ZConf)) {
			$YG.ZConf = {spaceid:__SpaceID, tracked_mods:{'ytm_nav':'nav'},pageview_on_init:true,client_only:1,keys:{pt:RapidPT}}; 
		} else {
			$YG.ZConf.apv = false;
		}
		new YAHOO.i13n.Rapid($YG.ZConf);
	},
	'MakeComScore': function() {
        var comscoreData = {
            c1: "2",
            c2: "7241469",
            c5: __SpaceID,
            c7: escape(decodeURI(window.location.href))
        };
        window.__adlib.fireComscoreBeacon(comscoreData);
	},
    MakeDotQ: function() {
		var tags = '';
		if (RapidPT === 4) {
            if (typeof(Tags) != 'undefined' && Array.isArray(Tags)) {
                tags = Tags.join();
            } else {
                tags = '';
            }
		}
        window.dotq = window.dotq||[];
		window.dotq.push({
			'projectId':'10000',
			'properties':{
				'pixelId':'428726',
				'qstrings':{
					'country':'tw', 
					'property':'games',
                    'spaceid':__SpaceID,
					'tag': tags
				}
			}
		});
		$.getScript('https://s.yimg.com/wi/ytc.js', function() {
			YAHOO.ywa.I13N.fireBeacon(window.dotq);
			window.dotq = [];
			window.dotq.push=function(p) { YAHOO.ywa.I13N.fireBeacon([p]); };
		});  
    },

};
